    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.4",
        "@types/node": "^20.10.6",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3"
    },
    "keywords": [
        "brutalist-ui",
        "cli",
        "neo-brutalism",
        "react",
        "components"
    ],
    "author": "dev-snake",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/dev-snake/brutalist-ui"
    },
    "homepage": "https://brutalistui.site"
