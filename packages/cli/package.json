{
    "name": "brutalist-ui-cli",
    "version": "0.1.3",
    "description": "CLI for adding Brutalist UI components to your project",
    "bin": {
        "brutalist-ui": "dist/index.js"
    },
    "main": "./dist/index.js",
    "type": "module",
    "files": [
        "dist",
        "templates"
    ],
    "scripts": {
